{
  "name": "pluralsight-js-dev-env",
  "version": "1.0.0",
  "description": "Javascript development environment Pluralsight course by Cory House",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch start-mockapi",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint": "esw webpack.config.* src buildScripts",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickygubatina/pluralsight-js-dev-env.git"
  },
  "author": "Ricky Gubatina",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rickygubatina/pluralsight-js-dev-env/issues"
  },
  "homepage": "https://github.com/rickygubatina/pluralsight-js-dev-env#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "cheerio": "^1.0.0-rc.2",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-watch": "^3.1.4",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "jsdom": "^11.10.0",
    "json-schema-faker": "^0.5.0-rc15",
    "json-server": "^0.12.2",
    "mocha": "^5.1.1",
    "nock": "^9.2.5",
    "npm-run-all": "^4.1.3",
    "nsp": "^3.2.1",
    "numeral": "^2.0.6",
    "open": "0.0.5",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.1",
    "webpack-dev-middleware": "^3.1.3",
    "whatwg-fetch": "^2.0.4"
  },
  "dependencies": {}
}
